<h2>Account Settings - Lawyer</h2>

<%= simple_form_for(@profile, url: update_account_settings_path) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :first_name, autofocus: true %>
  </div>
  
  <div class="form-inputs">
    <%= f.input :last_name %>
  </div>
  
  <div class="form-inputs">
    <%= f.input :company_name %>
  </div>
  
  <div class="form-inputs">
    <%= f.input :mobile_no %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Update" %>
  </div>
<% end %>
